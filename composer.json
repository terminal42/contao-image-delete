{
    "name": "terminal42/contao-image-delete",
    "description": "Safe-delete images by also removing generated assets",
    "keywords": ["delete", "images", "license", "secure"],
    "type": "contao-bundle",
    "license": "MIT",
    "authors": [{
        "name": "terminal42 gmbh",
        "homepage": "https://www.terminal42.ch"
    }],
    "funding": [{
        "type": "github",
        "url": "https://github.com/terminal42"
    }, {
        "type": "other",
        "url": "https://ko-fi.com/terminal42"
    }],
    "support": {
        "issues": "https://github.com/terminal42/contao-image-delete/issues",
        "source": "https://github.com/terminal42/contao-image-delete",
        "forum": "https://community.contao.org"
    },
    "require": {
        "php": "^7.4 || ^8.0",
        "contao/core-bundle": "^4.9",
        "symfony/config": "^4.4 || ^5.0",
        "symfony/dependency-injection": "^4.4 || ^5.0",
        "symfony/event-dispatcher": "^4.4 || ^5.0",
        "symfony/http-kernel": "^4.4 || ^5.0",
        "doctrine/dbal": "^2.11"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0",
        "contao/easy-coding-standard": "^3.0"
    },
    "conflict": {
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "autoload": {
        "psr-4": {
            "Terminal42\\ImageDeleteBundle\\": "src/"
        }
    },
    "extra": {
        "contao-manager-plugin": "Terminal42\\ImageDeleteBundle\\ContaoManager\\Plugin"
    },
    "scripts": {
        "cs-fixer": [
            "vendor/bin/ecs check src/ --fix --ansi"
        ]
    }
}
